{
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "api/index.php" },
      { "src": "/(.*)", "dest": "api/index.php" }
    ],
    "rewrites": [
      {
        "source": "/styles.css",
        "destination": "/styles.css",
        "headers": { "Content-Type": "text/css" }
      }
    ]
  }
  